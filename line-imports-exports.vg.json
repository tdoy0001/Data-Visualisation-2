{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Imports and exports",
  "height": 280,
  "width": 330,
  "data": {
    "url": "data/Imports-Exports.csv"
  },
  "mark": "line",
  "transform": [
    {
      "filter": "datum['Total value ($1000)'] > 0"
    }
  ],
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal"
    },
    "y": {
      "field": "Total value ($1000)",
      "type": "quantitative",
      "scale": {
        "type": "log"
      },
      "axis": {"tickCount": 7}
    },
    "color": {
      "field": "Type",
      "type": "nominal",
      "scale": {
        "domain": [
          "Export",
          "Import"
        ],
        "range": [
          "#ff7f0e",
          "#1f77b4"
        ]
      }
    },
    "tooltip": [
          {"field": "Type", "type": "nominal"},
          {"field": "Year", "type": "temporal", "format": "%Y" },
          {"field": "Total value ($1000)", "type": "quantitative", "format": ","},
          {"field": "Value per litre (cents)", "type": "quantitative", "format": ","}
    ]
  }
}